module samples.mojom;

interface SlaveControl {
  // Tell the slave process that it's safe to shutdown.
  ProcessShutdown();

  // Tell the slave process to begin or end IPC message logging.
  [EnableIf=ipc_logging]
  SetIPCLoggingEnabled(bool on);
};
